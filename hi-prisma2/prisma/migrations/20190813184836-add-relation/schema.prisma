datasource db {
  provider = "postgresql"
  url      = "postgresql://prisma2:prisma2@localhost:8300/prisma2?schema=public"
}

generator photonjs {
  provider = "photonjs"
}

model User {
  id    String  @id @default(cuid())
  name  String?
  email String  @unique
  posts Post[]
}

model Post {
  id        String  @id @default(cuid())
  title     String
  content   String?
  author    User?
  published Boolean @default(false)
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
}
